<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="aplicacion.modelo.dominio.Medicamento" catalog="veterinaria1" table="medicamento" optimistic-lock="version">
        <id name="idMedicamento" type="integer">
            <column name="id_medicamento" length="12"/>
            <generator class="assigned"/>
        </id>
        <property name="nombreMedicamento" type="string">
            <column name="nomb_medicamento" length="100" not-null="true"/>
        </property>
        <property name="fechaVencimiento" type="date">
            <column name="fech_venc_medicamento" not-null="true"/>
        </property>
        <property name="descripcionMedicamento" type="string">
            <column name="descr_medicamento" length="200" not-null="true"/>
        </property>
        <property name="estado" type="boolean">
            <column name="medcmento_estado" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
